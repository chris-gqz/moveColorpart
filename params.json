  "body": "<!DOCTYPE html>\r\n<html>\r\n\t<head>\r\n\t\t<meta charset=\"utf-8\" />\r\n\t\t<title></title>\r\n\t\t<style type=\"text/css\">\r\n\t\tbody,div{margin: 0px;padding: 0px;}\r\n\t\tbody{position: relative;}\r\n\t\t/*过渡*/\r\n\t\t.smooth { -webkit-transition-duration:300ms; }\r\n\t\tdiv{position: absolute; width: 100px;height: 100px;border: 1px solid black;line-height: 100px;text-align: center;font-size: 20px;color: white;\tbox-sizing: border-box;}\r\n\t\t#Focus{\r\n\tposition: absolute;\r\n\twidth: 100px;\r\n\theight: 100px;\r\n\tborder: 3px #abe430 solid;\r\n\t/*display: none;*/\r\n}\r\n\t\t#menuFocus{\r\n\tposition: absolute;\r\n\twidth: 100px;\r\n\theight: 100px;\r\n\tborder: 3px #abe430 solid;\r\n\tdisplay: none;\r\n}\r\n\t\t</style>\r\n\t</head>\r\n\t\r\n\t<body>\r\n\t\t<div id=\"menu\" style=\"border: none;\">\r\n\t\t\t<a style=\"display: block; position: absolute;top: 10px;left: 50px;width: 100px;height: 30px;background-color:#3086b7 ;text-align: center;line-height: 30px;color: white;\">精选</a>\r\n\t\t\t<a style=\"display: block;  position: absolute;top: 10px;left: 200px;width: 100px;height: 30px;background-color:#41a332; text-align: center;line-height: 30px;color: white;\">儿歌</a>\r\n\t\t\t<a style=\"display: block;  position: absolute;top: 10px;left: 350px;width: 100px;height: 30px;background-color:#7fd3ff ;text-align: center;line-height: 30px;color: white;\">流行</a>\r\n\t\t    <div id=\"menuFocus\" class=\"smooth\"> </div>\t\r\n\t\t</div>\r\n\t\t<div id=\"list1\" style=\"border: none;position: absolute;top: 0;left: 0;display:block;\">\r\n\t\t\t<div id=\"div1\" style=\"background-color:#3086b7 ;top: 50px;left: 50px;width: 200px;height: 200px;\">1-1</div>\r\n\t\t\t<div id=\"div2\" style=\"background-color:#b3d330 ;top: 50px;left: 300px;width: 100px;height: 200px;\">1-2 </div>\r\n\t\t\t<div id=\"div3\" style=\"background-color:#35b9bf ;top: 50px;left: 450px;width: 100px;height: 200px;\"> 1-3</div>\r\n\t\t\t<div id=\"div4\" style=\"background-color:#41a332